{
    "env": {
        "browser": true
    },
    "rules": {
        "space-unary-ops": 0,
        "strict": 0,
        "no-console": 1,
        "new-cap": 0,
        "curly": 0,
        "no-unused-vars": 1,
        "dot-notation": 1,
        "no-redeclare": 0,
        "no-shadow": 1,
        "no-trailing-spaces": 0,
        "no-empty": 0,
        "no-labels": 0
    },
    "globals": {
        "Class": true,
        "define": true,
        "PIXI": true,
        "WebFont": true,
        "Log": true
    }
}
